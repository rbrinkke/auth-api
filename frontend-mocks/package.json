    "msw": "^2.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "msw": "^2.0.0",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.3",
    "typescript": "^5.0.0",
    "uuid": "^9.0.1",
    "vitest": "^1.0.0"
  },
  "scripts": {
